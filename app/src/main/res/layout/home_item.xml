<?xml version="1.0" encoding="utf-8"?>
<com.kproduce.roundcorners.RoundLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/dp_100"
    android:layout_height="@dimen/dp_120"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:padding="@dimen/dp_10"
    android:layout_margin="@dimen/dp_10"
    app:rRadius="@dimen/dp_10"
    android:background="#ffffff"
    android:id="@+id/home_item_group"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/home_itme_icon"
        android:src="@mipmap/logo_jiu_black_mini"
        android:layout_width="@dimen/dp_40"
        android:layout_height="@dimen/dp_40"
        android:layout_gravity="center">
    </ImageView>

    <TextView
        android:id="@+id/home_item_name"
        android:text="name"
        android:maxLines="2"
        android:gravity="center"
        android:textColor="#888888"
        android:textSize="@dimen/sp_12"
        android:textStyle="bold"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </TextView>

</com.kproduce.roundcorners.RoundLinearLayout>